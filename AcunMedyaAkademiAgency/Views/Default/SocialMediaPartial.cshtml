@model List<AcunMedyaAkademiAgency.Entities.SocialMedia>
<style>
    .btn-social img {
        max-width: 100%;
        max-height: 100%;
        width: auto;
        height: auto;
    }
</style>

@foreach (var item in Model)
{
    <a class="btn btn-dark btn-social mx-2" href="@item.Url">
        <img class="rounded-circle img-fluid" src="@item.icon" alt="..." />
    </a>
}